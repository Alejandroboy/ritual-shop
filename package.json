{
  "name": "ritual-shop",
  "version": "1.0.0",
  "description": "Ritual shop on Next.js, Nest.js, Postgress and Tailwind",
  "main": "index.js",
  "author": "Alejandro <alexander.borodulin@alejandrogroup.org",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:all": "turbo run dev --parallel",
    "build:all": "turbo run build",
    "typecheck:all": "turbo run typecheck",
    "lint:all": "turbo run lint",
    "test:all": "turbo run test",
    "prepare": "husky",
    "prettier-format": "prettier --config .prettierrc '**/*.{json,js,jsx,ts,tsx,css,scss,md}' --write"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "husky-init": "^8.0.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "devDependencies": {
    "@nestjs/config": "^4.0.2",
    "husky": "^9.1.7"
  }
}
